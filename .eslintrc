{
  "extends": ["eslint:recommended", "prettier", "plugin:jest/recommended"],
  "plugins": ["react", "prettier", "jest"],
  "env": {
    "browser": true,
    "commonjs": true,
    "node": true,
    "es6": true
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2018,
    "ecmaFeatures": {
      "jsx": true
    },

    "sourceType": "module"
  },
  "rules": {
    "no-console": "off",
    "strict": ["error", "global"],
    "curly": "warn",
    "prettier/prettier": "error",
    "react/prop-types": 0
  }
}
